{
	"compilerOptions": {
		"target": "ES2015",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"types": [],
		"allowJs": false,
		"skipLibCheck": false,
		"allowSyntheticDefaultImports": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"noEmit": true,
		"jsx": "react-jsx"
	},
	"include": ["src"]
}
